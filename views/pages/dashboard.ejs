<!DOCTYPE html>
<html>

<head>
  <%- include('../layouts/head') %>
</head>

<body class="hold-transition skin-blue sidebar-mini">
  <style type="text/css">
    .message {
      color: #3c763d;
      background-color: #dff0d8;
      border-color: #d6e9c6;
      padding: 15px;
      margin-bottom: 20px;
      border: 1px solid transparent;
      border-radius: 4px;
    }
  </style>
  <div class="wrapper">
    <%- include('../layouts/header') %>

      <div class="content-wrapper">

        <section class="content">
          <div class="box box-primary">
            <div class="box-header with-border">
              <h3 class="box-title">Enquiry Listing</h3>
            </div>

            <div class="box-body table-responsive">
              <table id="enquiry-list" class="table table-striped table-bordered" style="width:100%">
                <thead>
                  <tr>
                    <th>Name</th>
                    <th>Mobile</th>
                    <th>EmailID</th>
                    <th>Status</th>
                  </tr>
                </thead>
                <tbody>

                </tbody>
              </table>
            </div>

          </div>
        </section>
      </div>

      <%- include('../layouts/sidebar') %>

  </div>
  <%- include('../layouts/footer') %>
    <!--canvas Chart-->
    <script type="text/javascript">
      let dataTable = '';
      $(document).ready(function () {
        dataTable = $("#enquiry-list").DataTable({
          "ajax": {
            url: "/dashboard/enquiry-data-table",
            type: 'GET'
          }
        });
      })
    </script>

</body>

</html>